                          &quot;BTT&quot;: {
                            &quot;address_offset&quot;: &quot;0x28&quot;,
                            &quot;size&quot;: 32,
                            &quot;display_name&quot;: &quot;CDMA Bytes To Transfer Register&quot;,
                            &quot;description&quot;: &quot;CDMA Bytes To Transfer Register&quot;,
                            &quot;is_volatile&quot;: true,
                            &quot;access&quot;: &quot;read-write&quot;,
